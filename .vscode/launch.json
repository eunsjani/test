{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Web Hot Reload",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "deviceId": "chrome",
            "args": [
                "--web-port=8080",
                "--web-renderer=html"
            ]
        }
    ]
}